BEGIN
CREATE TABLE sub_tx_table0a (c int);
INFO:  DDL statement replicated.
CREATE TABLE
ROLLBACK;
ROLLBACK
\d sub_tx_table0a
Did not find any relation named "sub_tx_table0a".
EXECUTE spocktab('sub_tx_table0a');
